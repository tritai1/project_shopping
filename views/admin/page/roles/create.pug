extends ../../layout/layout.pug
block mains
  main.content
    h1 Tạo mới danh mục
    form(action=`${variableAll}/rolePower/creates` method="POST")
        div.form-group
            label(for="title") Tiêu đề
            input#title(type="text" name="title" placeholder="Nhập tiêu đề" class="form-control")
        div.form-group
            label(for="description") Mô tả
            textarea#description(name="description" rows="4" placeholder="Nhập mô tả" class="form-control")
        div.form-group
            button(type="submit" class="btn btn-primary") Tạo mới
    